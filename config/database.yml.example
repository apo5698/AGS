default: &default
  adapter: postgresql
  host: localhost
  encoding: unicode
  pool: <%= ENV.fetch('WEB_CONCURRENCY', 2).to_i * ENV.fetch('RAILS_MAX_THREADS', 5).to_i %>
  username:
  password:

development:
  <<: *default
  database: ags_development

test:
  <<: *default
  database: ags_test

production:
  <<: *default
  database: ags
