<div class="list-group-item-action d-flex card-header" id="heading_<%= seq %>" data-toggle="collapse" data-target="#collapse_<%= seq %>">
  <span id="seq"><%= seq %></span>. Student BBT
</div>
<div id="collapse_<%= seq %>" class="collapse" aria-labelledby="heading_<%= seq %>" data-parent="#rubric_items_accordion">
  <div class="card-body">
    <%= render partial: '/rubric_items/item', locals: {usage: 'This step grants an interactive web console for grading the student\'s black box tests.',
                                                       required_items: %w(input_file oracle_file bbtp description type points criterion response),
                                                       default_set: RubricItem.default_set(:student_bbt),
                                                       default_description: RubricItem.default_description(:student_bbt)} %>
  </div>
</div>