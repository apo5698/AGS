<h2>Preview</h2>
<div style="overflow-x: auto">
  <table class="table table-sm table-hover table-striped table-bordered">
    <thead>
    <tr class="table-active">
      <% ReportsHelper.header(@csv_export).each do |col| %>
        <th scope="col"><%= col %></th>
      <% end %>
    </tr>
    </thead>
    <tbody>
    <% @csv_export&.each do |row| %>
      <tr>
        <% row.each do |col| %>
          <td style="white-space: nowrap"><%= col[1] %></td>
        <% end %>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
<div class="float-right mt-3">
  <%= button_to('Download', { action: :download }, { class: 'btn btn-primary', method: :get }) %>
</div>