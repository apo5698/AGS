<h2>Preview</h2>
<div style="overflow-x: auto">
  <table class="table table-hover">
    <thead>
    <tr>
      <% ReportsHelper.header(@csv_in).each do |col| %>
        <th scope="col"><%= col %></th>
      <% end %>
    </tr>
    </thead>
    <tbody>
    <% @csv_in&.each do |row| %>
      <tr>
        <% row.each do |col| %>
          <td style="white-space: nowrap"><%= col[1] %></td>
        <% end %>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>