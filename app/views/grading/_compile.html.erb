<div class="row" style="margin-bottom: 1.5em">
  <div class="col-lg-12">
    <h1>Compile</h1>
  </div>
</div>
<div class="row" style="margin-bottom: 2em">
  <div class="col-lg-5">
    <h3>Source Files</h3>
    <ul class="list-group">
      <li class="list-group-item d-flex justify-content-between align-items-center code">
        Box.java
        <span class="badge badge-success badge-pill">
          <i class="fas fa-check"></i>
        </span>
      </li>
      <li class="list-group-item d-flex justify-content-between align-items-center active code">
        BoxList.java
        <span class="badge badge-dark badge-pill">12</span>
      </li>
      <li class="list-group-item d-flex justify-content-between align-items-center code">
        BoxUI.java
        <span class="badge badge-primary badge-pill">7</span>
      </li>
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <h3>Console Output</h3>
    <div class="terminal overflow-auto">
      <div class="terminal-content"><%= File.open('app/views/grading/sample_compile_output.txt').read %>
      </div>
    </div>
  </div>
</div>
